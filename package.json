{
  "name": "dientasoft",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "serve": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:web": "cross-env RUN_MODE=web ng serve",
    "start:electron": "NODE_ENV=development electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "start:dev": "concurrently \"npm run start:web\" \"electron .\"",
    "ng": "ng",
    "electron-build": "cross-env NODE_ENV=production electron .",
    "dev": "concurrently \"npm run start\" \"npm run electron\"",
    "start": "ng serve",
    "electron": "wait-on http://localhost:4200 && electron .",
    "electron-dev": "cross-env NODE_ENV=development npm run electron",
    "electron-prod": "cross-env NODE_ENV=production electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "chart.js": "^4.3.0",
    "electron-squirrel-startup": "^1.0.1",
    "luxon": "^3.4.4",
    "ng2-charts": "^5.0.4",
    "ngx-material-timepicker": "^13.1.1",
    "rxjs": "~7.8.0",
    "selenium-webdriver": "^4.21.0",
    "sweetalert2": "^11.14.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "main": "dist/mainElectron.js",
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.7",
    "@angular/cli": "^16.2.7",
    "@angular/compiler-cli": "^16.2.0",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "@types/selenium-webdriver": "^4.1.23",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^33.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~4.6.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.35",
    "ts-jest": "^29.1.4",
    "typescript": "~5.1.3",
    "wait-on": "^8.0.1"
  }
}
